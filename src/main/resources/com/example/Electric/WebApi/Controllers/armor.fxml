<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.lang.Integer?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="691.0"
            prefWidth="361.0" style="-fx-background-image:url('background.png');-fx-background-size: cover;"
            xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.Electric.WebApi.Controllers.ArmorController">
    <children>
        <fx:define>
            <Integer fx:id="min" fx:value="0"/>
            <Integer fx:id="max" fx:value="100"/>
        </fx:define>
        <Label layoutX="147.0" text="Armor"/>
        <Label layoutX="153.0" layoutY="59.0" text="Name"/>
        <Label layoutX="106.0" layoutY="151.0" text="Take damage"/>
        <Label layoutX="165.0" layoutY="247.0" text="Size"/>
        <TextField fx:id="nameField" layoutX="115.0" layoutY="118.0"/>
        <CheckBox fx:id="ceremonialField" layoutX="121.0" layoutY="355.0" mnemonicParsing="false"
                  style="-fx-font-size: 20; -fx-font-weight: bold;" text="Ceremonial"/>
        <ChoiceBox fx:id="sizeField" layoutX="114.0" layoutY="299.0" minHeight="-Infinity" minWidth="-Infinity"
                   prefHeight="40.0" prefWidth="150.0"/>
        <Button layoutX="91.0" layoutY="606.0" mnemonicParsing="false" text="Add armor" onAction="#addArmor"/>
        <Spinner fx:id="damageField" layoutX="115.0" layoutY="215.0" min="$min" max="$max"/>
        <Spinner fx:id="priceField" layoutX="115.0" layoutY="455.0" min="$min" max="$max"/>
        <Label layoutX="157.0" layoutY="401.0" text="Price"/>
        <Spinner fx:id="weightField" layoutX="119.0" layoutY="545.0" min="$min" max="$max"/>
        <Label layoutX="147.0" layoutY="493.0" text="Weight"/>

    </children>
</AnchorPane>
